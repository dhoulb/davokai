{
	"name": "davokai",
	"displayName": "Davokai",
	"description": "Monokai-based color theme for VSCode with Dave improvements.",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.78.0"
	},
	"categories": [
		"Themes"
	],
	"contributes": {
		"themes": [
			{
				"label": "Davokai Dark",
