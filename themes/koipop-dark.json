	"tokenColors": [
		{
			"name": "Gray: comments, e.g. `// My comment.` and disabled elements",
			"scope": ["comment", "punctuation.definition.comment"],
			"settings": {
				"foreground": "#abb1ba"
			}
		},
		{
			"name": "Yellow: strings, e.g. `'abc'`",
			"scope": ["string"],
			"settings": {
				"foreground": "#ffe01a"
			}
		},
		{
			"name": "Green: functions and classes, e.g. `myVar()` and `MyClass`",
			"scope": ["entity.name.function", "meta.function-call", "variable.function", "support.function", "keyword.other.special-method", "meta.class-method.js entity.name.function.js", "variable.function.constructor"],
			"settings": {
				"foreground": "#92f416"
			}
		},
		{
			"name": "Orange:",
			"scope": [],
			"settings": {
				"foreground": "#ffb21a"
			}
		},
		{
			"name": "Purple: numbers and constants, e.g. `123` and `null`",
			"scope": [],
			"settings": {
				"foreground": "#9f49c7"
			}
		},
		{
			"name": "Pink: keywords, e.g. `export`, `const`, `function`, `readonly`, `private`",
			"scope": [],
			"settings": {
				"foreground": "#e718a2"
			}
		},
		{
			"name": "Blue: basic types, e.g. `: string`, and interfaces and classes used as types, e.g. `: MyInterface`",
			"scope": [],
			"settings": {
				"foreground": "#2f78e5"
			}
		},
		{
			"name": "Red: errors",
			"scope": ["invalid", "invalid.illegal"],
			"settings": {
				"foreground": "#e5352f"
			}
		},
		{}
	]
}
