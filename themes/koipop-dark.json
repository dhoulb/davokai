	"semanticHighlighting": true,
	"semanticTokenColors": {
		"string": "#ffb21a",
		"regexp": "#ffb21a",
		"number": "#f047ff",
		"class": "#ffe01a",
		"decorator": "#92f416",
		"function": "#92f416",
		"method": "#92f416",
		"type": "#15f4e5",
		"interface": "#15f4e5",
		"typeParameter": "#15f4e5",
		"parameter": "#ffffff",
		"property": "#ffffff",
		"variable": "#ffffff"
	},
	"tokenColors": [
		{
			"name": "Orange: strings e.g. `'abc'`",
			"scope": ["string", "markup.inline.raw"],
			"settings": {
				"foreground": "#ffb21a"
			}
		},
		{
						"name": "Green: functions e.g. `myFunc()`",
			"scope": ["entity.name.function", "support.function"],
			"settings": {
				"foreground": "#92f416"
			}
		},
		{
			"name": "Yellow: classes e.g. `new MyClass()`",
			"scope": ["entity.name.type.class", "entity.other.inherited-class", "variable.language.this", "variable.language.super", "markup.bold", "markup.italic", "entity.name.tag"],
			"settings": {
				"foreground": "#ffe01a"
			}
		},
		{
			"name": "Purple: numbers e.g `123`, constants e.g. `null`",
			"scope": ["constant.numeric", "constant.language", "support.constant"],
			"settings": {
				"foreground": "#f047ff"
			}
		},
		{
			"name": "Pink: keywords e.g. `export` or `readonly` or `private`, declarations e.g. `const` and `function`",
			"scope": ["keyword.control", "keyword.operator.new", "storage.modifier", "storage.type", "entity.other.attribute-name.pseudo-class"],
			"settings": {
				"foreground": "#f43789"
			}
		},
		{
			"name": "Cyan: primitive types e.g. `string`, complex types e.g. `MyType`",
			"scope": ["support.type.builtin", "support.type.primitive", "entity.name.type", "punctuation.definition.heading", "string.other.link.title", "entity.other.attribute-name"],
			"settings": {
				"foreground": "#15f4e5"
			}
		},
		{
			"name": "Dark cyan: primitive types e.g. `string`, complex types e.g. `MyType`",
			"scope": ["markup.underline.link"],
			"settings": {
				"foreground": "#15f4e588"
			}
		},
		{
			"name": "Blue: info",
			"scope": [],
			"settings": {
				"foreground": "#2f78e5"
			}
		},
		{
			"name": "Red: errors",
			"scope": [],
			"settings": {
				"foreground": "#e5352f"
			}
		},
		{
			"name": "White: variables, object keys, markup",
			"scope": ["variable", "support.type.property-name", "entity.name.label", "meta.object-literal.key string", "markup", "markup string", "meta.paragraph", "constant.other.color"],
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"name": "Grey: punctuation, comments",
			"scope": ["comment", "punctuation", "storage.type.function.arrow", "meta.brace"],
			"settings": {
				"foreground": "#abb1ba"
			}
		},
		{
			"name": "Bold: bold markup",
			"scope": ["markup.bold"],
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"name": "Italic: bold markup, type annotations e.g. `x: string`",
			"scope": ["meta.type.annotation", "meta.return.type", "markup.italic"],
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"name": "Underline: URLs",
			"scope": ["*url*", "*link*", "*uri*"],
			"settings": {
				"fontStyle": "underline"
			}
		}
	]
}
